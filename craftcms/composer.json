{
    "config": {
        "type": "project",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "8.2",
            "ext-bcmath": "8.2",
            "ext-intl": "8.2",
            "ext-bz2": "8.2",
            "ext-soap": "8.2",
            "ext-sodium": "8.2",
            "ext-zip": "8.2"
        },
        "allow-plugins": {
            "yiisoft/*": true,
            "craftcms/*": true,
            "treeware/plant": true,
            "php-http/discovery": true,
            "craftcms/plugin-installer": true,
            "yiisoft/yii2-composer": true
        }
    },
    "prefer-stable": true,
    "minimum-stability": "beta",
    "require": {
        "craftcms/ckeditor": "^4.2.0",
        "craftcms/cms": "^5.4.7.1",
        "craftcms/feed-me": "^6.4.1",
        "embed/embed": "^4.4.12",
        "mmikkel/retcon": "^3.2.0",
        "behinddesign/craft-object-storage": "^1.0.0",
        "nystudio107/craft-emptycoalesce": "^5.0.0",
        "nystudio107/craft-seomatic": "^5.1.4",
        "nystudio107/craft-vite": "^5.0.1",
        "openai-php/client": "^0.8.5",
        "simplicateca/selectplus": "^5.0.5-beta",
        "verbb/formie": "^3.0.8",
        "twig/html-extra": "^3.13.0",
        "verbb/navigation": "^3.0.4",
        "vlucas/phpdotenv": "^5.6.1",
        "yiisoft/yii2-redis": "^2.0.18"
    },
    "require-dev": {
        "craftcms/generator": "^2.1.0",
        "yiisoft/yii2-shell": "^2.0.5"
    },
    "autoload": {
        "psr-4": {
            "modules\\sitemodule\\": "modules/sitemodule/src/",
            "modules\\themecore\\": "modules/themecore/src/"
        }
    }
}
